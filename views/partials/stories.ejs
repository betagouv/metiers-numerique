<section class="fr-py-6w">
  <div class="fr-container">
    <div class="fr-fr-grid-row fr-grid-row--center rf-centered">
      <div>
        <h2><%= stories.title %></h2>
        <span><%= stories.subtitle %></span>
      </div>
    </div>
  </div>
  <div class="fr-container">
    <div class="fr-grid-row fr-grid-row--gutters fr-mt-4v fr-pb-4v">
      <div class="fr-col-12 fr-col-md-4 fr-mb-3w">
        <div class="fr-grid-row rf-centered fr-mx-3w">
          <img src="<%= stories.parts[0].src %>" alt="<%= stories.parts[0].alt %>">
        </div>
      </div>
    </div>
  </div>
</section>