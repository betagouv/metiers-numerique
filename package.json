    "ace-builds": "1.4.13",
    "apollo-server-micro": "3.6.2",
    "aws-sdk": "2.1054.0",
    "better-prop-types": "1.0.0",
    "bhala": "2.1.0",
    "cuid": "2.1.8",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "download": "8.0.0",
