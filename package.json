{
  "name": "metiers-numeriques",
  "description": "Tout savoir sur les métiers du numérique au sein de l’État.",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "scripts": {
    "prebuild": "sh ./scripts/build/prebuild.sh",
    "build": "next build",
    "postbuild": "sh ./scripts/build/postbuild.sh",
    "db:backup": "sh ./scripts/dev/backup.sh",
    "db:migrate": "prisma migrate deploy",
    "db:restore": "sh ./scripts/dev/restore.sh",
    "db:seed": "prisma db seed",
    "dev": "next dev",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.yml -f ./docker-compose.dev.yml up -d cache db",
    "dev:migrate": "prisma migrate dev",
    "dev:seed": "ts-node -r dotenv/config ./scripts/dev/seed.ts && yarn db:seed",
    "dev:worker:bot": "node -r dotenv/config ./workers/bot/dev.mjs",
    "dev:worker:pep": "node -r dotenv/config ./workers/pep/dev.mjs",
    "prepare": "husky install && sh ./scripts/build/generateAssets.sh",
    "setup": "sh ./scripts/dev/setup.sh",
    "start": "npx next start",
    "start:worker:bot": "node -r dotenv/config ./workers/bot/cron.mjs",
    "start:worker:pep": "node -r dotenv/config ./workers/pep/cron.mjs",
    "test": "yarn test:lint && yarn test:build && yarn test:unit",
    "test:lint": "eslint --ext js,mjs,ts,tsx .",
    "test:e2e": "playwright test -c ./config/playwright.config.ts",
    "test:type": "tsc",
    "test:unit": "jest --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "yarn test:unit --watch"
  },
  "prisma": {
    "seed": "ts-node -r dotenv/config prisma/seeds/index.ts"
  },
