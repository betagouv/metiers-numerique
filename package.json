{
  "name": "metiers-numeriques",
  "description": "Tout savoir sur les métiers du numérique au sein de l’État.",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "scripts": {
    "prebuild": "sh ./scripts/build/prebuild.sh",
    "build": "yarn build:assets && next build",
    "build:assets": "rm -Rf ./public/assets && rollup -c && rm -f ./public/index.js",
    "build:sitemap": "ts-node -r dotenv/config ./scripts/build/generateSitemap.ts",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "dev": "next dev",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.yml -f ./docker-compose.dev.yml up -d db",
    "dev:migrate": "prisma migrate dev",
    "prepare": "husky install",
    "setup": "yarn nexauth init",
    "start": "npx next start",
    "test": "yarn test:lint && yarn test:build && yarn test:unit",
    "test:lint": "eslint --ext js,ts,tsx .",
    "test:e2e": "playwright test -c ./config/playwright.config.ts",
    "test:type": "tsc",
    "test:unit": "jest --config=./config/jest.unit.config.js",
    "test:unit:watch": "yarn test:unit --watch"
  },
  "prisma": {
    "seed": "ts-node -r dotenv/config prisma/seeds/index.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.7",
    "@gouvfr/dsfr": "1.0.0-rc1.0",
