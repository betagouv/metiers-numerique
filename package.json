{
  "name": "metiers-numeriques",
  "description": "Tout savoir sur les métiers du numérique au sein de l’Etat",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "16",
    "npm": "7"
  },
  "scripts": {
    "checkHTML": "site-validator --verbose --url",
    "checkHTMLLocal": "site-validator --verbose --local --url http://localhost:8080",
    "dev:docker": "docker-compose down && docker-compose up -d cache",
    "dev": "nodemon index.js",
    "prepare": "husky install",
    "start": "node index.js",
    "test": "yarn test:unit && yarn test:integration",
    "test:commit": "commitlint --from 2c0f739 --to HEAD --verbose",
    "test:lint": "eslint --ext js .",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:unit": "jest --config=jest.config.js",
    "test:unit:watch": "jest --watchAll --config=jest.config.js"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.0.0-rc1.0",
    "axios": "0.21.4",
    "cron": "1.8.2",
    "csvtojson": "2.0.10",
    "date-fns": "2.25.0",
    "date-fns-tz": "1.1.6",
    "dotenv": "10.0.0",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "fuse.js": "6.4.6",
    "markdown-it": "12.2.0",
    "moment": "2.29.1",
    "redis": "3.1.2",
    "remixicon": "2.5.0",
    "string-strip-html": "8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@ivangabriele/eslint-config-base": "2.0.0",
    "@ivangabriele/prettier-config": "2.0.1",
    "@types/jest": "27.0.2",
    "husky": "7.0.4",
    "jest": "27.2.0",
    "nodemon": "2.0.6",
    "redis-mock": "0.56.3"
  },
  "prettier": "@ivangabriele/prettier-config",
  "bugs": {
    "url": "https://github.com/betagouv/metiers-numeriques/issues"
  },
  "homepage": "https://github.com/betagouv/metiers-numeriques#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/metiers-numeriques.git"
  }
}
